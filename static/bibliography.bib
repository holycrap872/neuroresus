@article{goostreyAdaptingTraumaticBrain2021,
  title = {Adapting a {{Traumatic Brain Injury Goals}}-of-{{Care Decision Aid}} for {{Critically Ill Patients}} to {{Intracerebral Hemorrhage}} and {{Hemispheric Acute Ischemic Stroke}}},
  author = {Goostrey, Kelsey J. and Lee, Christopher and Jones, Kelsey and Quinn, Thomas and Moskowitz, Jesse and Pach, Jolanta J. and Knies, Andrea K. and Shutter, Lori and Goldberg, Robert and Mazor, Kathleen M. and Hwang, David Y. and Muehlschlegel, Susanne},
