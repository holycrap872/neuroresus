  file = {/home/nikhil/Zotero/storage/H4HZ8QGJ/Bikdeli et al_Non-inferiority trials using a surrogate marker as the primary endpoint.pdf;/home/nikhil/Zotero/storage/HJW8G7JZ/Bikdeli et al_Non-inferiority trials using a surrogate marker as the primary endpoint.pdf;/home/nikhil/Zotero/storage/X6P3SK9S/Bikdeli et al_Non-inferiority trials using a surrogate marker as the primary endpoint.pdf}
