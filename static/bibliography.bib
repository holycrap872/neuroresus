@article{wijdicksTakingHistoryNeurocritically2020,
  ids = {wijdicksTakingHistoryNeurocritically,wijdicksTakingHistoryNeurocritically2020a},
  title = {Taking a {{History}} in {{Neurocritically Ill Patients}}},
